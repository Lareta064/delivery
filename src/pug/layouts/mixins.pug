mixin social-icons
	.social-icons&attributes(attributes)
		a(href="#")
			+telegram
		a(href="#")
			+viber
		a(href="#")
			+whatsapp
		a(href="#")
			+instagram
mixin counter
	.product-counter&attributes(attributes)
		span.product-counter--minus
		span.product-counter--num 1
		span.product-counter--plus
		input(type="hidden" name="product_count" value='1')

