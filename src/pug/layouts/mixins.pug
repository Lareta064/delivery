mixin radioBlock( itemId,text1,text2, radioName,  text3, check)
	.form-group&attributes(attributes)
		if check
			input(type="radio" id= itemId name= radioName checked)
		else
			input(type="radio" id= itemId name= radioName )
		label(for= itemId)
			span #{text1}
			span #{text2}
			span #{text3}
mixin social-icons
	.social-icons&attributes(attributes)
		a(href="#")
			+viber
		a(href="#")
			+telegram
		a(href="#")
			+whatsapp
		a(href="#")
			+instagram
mixin saleCard(imgName, periodDate, title, text)
	.col-md-6
		a(href="#!").sale-card
			span.card__badge  Акции
			.sale-card__img.lazy(data-src="img/"+ imgName +".jpg")
			.sale-card__desckrip
				.sale-card__period
					p Действует до&nbsp;#[span #{periodDate}]
				.sale-card__text
					h3.title-3 !{title}
					p !{text}
mixin basketItem
	.basket-item
		button.remove-item.remove-basket-item
			+times(30, 30)
		.row
			.col-4.col-md-3.col-lg-2
				.basket-item__img
					img(src="img/basket-product1.png", alt="product" width='170')
			.col-8.col-md-3.col-lg-3
				.basket-item__head
					a(href="product-card.html").title-34 
						span Пицца
						span “Особая”
					.basket-item__property
						.product-card__filters
							span.filter-name Тесто:
							.filters-type
								.filters-type__item
									label
										input(type="radio" name="product_testo" value="Традиционное" checked)
										.fake-radiobutton Традиционное
						.product-card__filters
							span.filter-name Pазмер:
							.filters-type
								.filters-type__item
									label
										input(type="radio" name="product_diameter" value="33" checked)
										.fake-radiobutton 33 см
			.col-md-6.col-lg-4.border-col
				.basket-item__descrip.text-16
					.basic-descrip
						p Говядина, халапеньо, моцарелла, помидоры, куриное филе, томатный соус.
					.addition-set
						button.remove-item.remove-addition
							+times(16, 16)
						p Дополнительно: Сыр моцарелла 50&nbsp;гр. Пепперони 30&nbsp;гр. Ветчина 30&nbsp;гр. Маслины&nbsp;20гр. Оливковое масло 20&nbsp;гр.
			.col-lg-3
				.basket-item__calc
					//- button.remove-item.remove-basket-item
					//- 	+times(30, 30)
					.basket-item__addInfo Вес: 550 гр
					.basket-item__summ
						+counter(class="product-counter--big")
						.basket-item__cost
							.basket-item__cost-val.title-34 650
							.basket-item__cost-dim руб

mixin main-product-card(title, imgSrc, weight, desk, price, oldPrice)
	.product-card&attributes(attributes)
		.card-badges
			span.card-badge.card-badge--sale Акция!
			span.card-badge.card-badge--new Новинка
			span.card-badge.card-badge--favorite Популярное
		form(action="#")
			input(type="hidden" name="product_name"  value=""+ title)
			input(type="hidden" name="product_weight" value="500 гр")
			.product-card__img
				img.lazy(data-src="img/main/"+ imgSrc +".png", alt="product" width="280")
			.product-card__title
				a(href="product-card.html" title=""+ title).product-title #{title}
				span.card-text #{weight} гр
			.product-card__deskrip
				
				p #{desk}
				.product-card__filters
					span.filter-name Размеры:
					.filters-type
						.filters-type__item
							label
								input(type= "radio" name="product_diameter" value="26см")
								.fake-radiobutton 26см
						.filters-type__item
							label
								input(type= "radio" name="product_diameter" value="33см")
								.fake-radiobutton 33см
						.filters-type__item
							label
								input(type= "radio" name="product_diameter" value="40см")
								.fake-radiobutton 40см
				.product-card__filters
					span.filter-name Тесто:
					.filters-type
						.filters-type__item
							label
								input(type="radio" name="product_testo" value="Традиционное")
								.fake-radiobutton Традиционное
						.filters-type__item
							label
								input(type="radio" name="product_testo" value="Тонкое")
								.fake-radiobutton Тонкое
			
			.product-card__addition
				.products-group__wrapper
					.products-group
						h4.products-group__title Соусы
						.products-group__List
							ul
								li
									+inputCheckbox('group1', 'Пепперони 40 гр.')
									span 50&#8381;
								li
									+inputCheckbox('group1', 'Красный лук 30 гр.')
									span 30&#8381;
								li
									+inputCheckbox('group1', 'Шампиньоны 30 гр.')
									span 50&#8381;
								li
									+inputCheckbox('group1', 'Перец Халапеньо 20 гр.')
									span 50&#8381;
					
						h4.products-group__title Соусы
						.products-group__List
							ul
								li
									+inputCheckbox('group2', 'Соус для пиццы 50 гр.')
									span 50&#8381;
								li
									+inputCheckbox('group2', 'Сливочно чесночный соус 50 гр.')
									span 50&#8381;
								li
									+inputCheckbox('group2', 'Соус Ранч 50 гр.')
									span 50&#8381;
								

				button.page-button--light.page-button.hide-back(type='button')
					+arrow

			.product-card-footer
				.product-card__calc
					.product-card__prises
						span.new-price
							span.value #{price}
							span.dim &#8381;
						if oldPrice
							span.old-price
								span.value 800
								span.dim &#8381;
					+counter
				
				.product-card__buttons
					button.page-button--light.page-button.show-back(type='button') Дополнительно
					.form-btn
						button.page-button--yellow.page-button Купить
						//- input(type="submit" value="Купить").page-button--yellow.page-button


